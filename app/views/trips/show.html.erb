<div class="ui container">
  <h1>Trip: <%= @trip.name %> </h1>
  <br/>
  <h3> Start Date: </h3>
  <h3><%=  @trip.start_date %> </h3>
  <br/>
  <h3> End Date: </h3>
  <h3><%=  @trip.end_date %></h3>
  <hr />
  <%= link_to "Locations", trip_locations_path(@trip) %>
  <%= link_to "Edit Trip", edit_trip_path %>
  <%= link_to "Delete Trip", [@trip], method: :delete %>
  <%= link_to "Back to Trips", trip_path %>
</div>

